<!DOCTYPE hml>

<html>

<head>

   <title>Int2</title>

</head>

<body>

 

   <script type="text/javascript">
     
     for(i=0; i<101; i++){
      f=i % 3;
      b=i % 5;

      if(f==0 $$ b==0){
        document.write("FizzBuzz ");
      }
      else if(f==0){
        document.write("Fizz ");
      }
      else if(b==0){
        document.write("Buzz ");
      }
      else {
        document.write(i+ " ");
      }
     }

   </script>
</body>

</html>